package alma.fr.logootenginecomponents;

import alma.fr.data.Positions;

public class MyDelta {

	private Operation type;
	private Positions id;
	private String content;

	public MyDelta(Operation type, Positions id, String content) {
		this.type = type;
		this.id = id;
		this.content = content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public void setId(Positions id) {
		this.id = id;
	}

	public void setType(Operation type) {
		this.type = type;
	}

	public Positions getId() {
		return id;
	}

	public Operation getType() {
		return type;
	}

	public String getContent() {
		return content;
	}

	@Override
	public String toString() {
		return "<" + type + "," + id + ">";
	}
}
